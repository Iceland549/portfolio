{
    "builds": [
      {
        "src": "/package.json",
        "use": "@vercel/static-build",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npx vite build",
          "nodeVersion": "16.x",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "headers": {
          "x-robots-tag": "noindex"
        }
      }
    ]
  }